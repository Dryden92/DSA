public class Main
{
    
	public static void main(String[] args) 
	{
	    int y=2023;
	    
	    if(y%4==0)
	    {
	        if(y%100==0)
	        {
	            if(y%400==0)
	            {
	                System.out.println("this is a leap year");
	            }
	            else
	            System.out.println("this is not a leap year");
	        }
	        else
	        System.out.println("this is a leap year");
	    }
	    else
	    	System.out.println("this is not a leap year");
	}
}
