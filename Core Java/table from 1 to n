public class Main
{
    void meth1(int a, int b)
    {
        for(int j=a;j<=b;j++)
        {
            for(int i=1;i<=10;i++)
              {
                System.out.println(j+" * "+i+" = "+j*i);
              }
              System.out.println("--------------------------");
        }
    }
	public static void main(String[] args) 
	{
		new Main().meth1(1,10);
	}
}
