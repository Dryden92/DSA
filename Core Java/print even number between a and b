public class Main
{
    void meth(int a, int b)
    {
        int j=0;
        for(int i=a;i<=b;i++)
        {
            if(i%2==0)
            {
                System.out.println(i);
                j++;
            }
        }
        System.out.println("total count= "+j);
    }
    public static void main(String [] args)
    {
        Main obj=new Main();
        obj.meth(1,100);
    }
}
