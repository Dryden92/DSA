
class Main 
{
    int i=10;
    static int y=20;
    
    void meth()
    {
        int i=2;
        int y=4;
        
        System.out.println("instance "+this.i);
        System.out.println("static "+Main.y);
    }
    public static void main(String[] args)
    {
        Main obj=new Main();
        obj.meth();
    }
}
