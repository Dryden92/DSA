//program for largest element in the array

public class Main
{
    int meth1(int arr[])
    {
      int big=arr[0];
      
        for(int i=1;i<arr.length;i++)
        {
            if(arr[i]>big)
            {
                big=arr[i];
            }
        }
        return big;
    }
	public static void main(String[] args) 
	{
	    int arr[] = {12, 35, 1, 10, 34, 61};
	    
		Main obj=new Main();
		int a=obj.meth1(arr);
		System.out.println(a);
	}
}
