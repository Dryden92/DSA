
class Main 
{
    static void meth(int arr[])
    {
        int n=arr.length;
        int max=arr[0];
        for(int i=0;i<n;i++)
        {
           if(arr[i]>max)
           {
               max=arr[i];
           }
        }
        System.out.println("greatest number is: "+max);
    }
    public static void main(String[] args) 
    {
       int arr[]={123, 2, 3, 4, 5, 621, 7, 8, 9, 10};
       
       meth(arr);
    }
}
