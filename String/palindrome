public class Main
{
	public static void main(String[] args) 
	{
	    
	    String s="abba";
	    
	    int n=s.length();
	    
	    String rev="";
	    
	    for(int i=0;i<n;i++)
	    {
	        rev=s.charAt(i)+rev;
	    }
	    if(s.equals(rev))
	    {
	        System.out.println("this is palindrome");
	    }
	    else
		{
		    System.out.println("not a palindrome");
		}
	}
}
